{
  "version": 2,
  "builds": [
      {
          "src": "run.py",
          "use": "@vercel/python",
          "config": {
              "runtime": "python3.9",
              "maxLambdaSize": "15mb",
              "buildCommand": "pip install -r requirements.txt"
          }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "run.py"
      }
  ],
  "env": {
      "PYTHONPATH": "."
  }
}