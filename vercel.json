{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python@3.1.30",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "buildCommand": "bash vercel_build.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ]
}